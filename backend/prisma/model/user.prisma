model User {
  id    Int     @id @default(autoincrement())
  firstName String
  lastName String
  email String  @unique
  password String
  role Role @default(USER)
  createdAt DateTime
  updatedAt DateTime
  order Order[]
  cart Cart?
  wishlist Wishlist?

  @@schema("user")
}


enum Role {
  USER
  ADMIN

  @@schema("user")
}