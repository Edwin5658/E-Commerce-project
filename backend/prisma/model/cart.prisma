model Cart {
    id Int  @id @default(autoincrement())
    user_id Int @unique
    product_id Int
    createdAt DateTime
    updatedAt DateTime
    cartItem CartItem[] 
    user User @relation(fields: [user_id], references: [id])

    @@schema("cart")
}

model CartItem {
    id Int @id @default(autoincrement())
    quantity Int
    cart_id Int
    product_id Int @unique
    createdAt DateTime
    updatedAt DateTime
    product Product?
    cart Cart @relation(fields: [cart_id], references: [id])

    @@schema("cart")
}