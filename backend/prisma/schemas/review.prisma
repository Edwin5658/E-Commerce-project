model Review {
    id        Int      @id @default(autoincrement())
    rating    Int
    comment   String?
    createdAt DateTime @default(now())

    user_id Int
    user    User @relation(fields: [user_id], references: [id])

    product_id Int
    product    Product @relation(fields: [product_id], references: [id])

    @@schema("review")
}
